{
  "id": "dev.akamfoad.passkeys",
  "name": "Pass keys",
  "short_name": "Pass keys",
  "lang": "en",
  "orientation": "portrait-primary",
  "description": "Passkey demonstration",
  "background_color": "#e5e5e5",
  "theme_color": "#020617",
  "display": "standalone",
  "display_override": [
    "standalone",
    "fullscreen",
    "browser",
    "window-controls-overlay",
    "minimal-ui"
  ],
  "shortcuts": [
    {
      "name": "Passkeys",
      "url": "/settings/passkeys",
      "description": "List of passkeys"
    }
  ],
  "handle_links": "not-preferred",
  "edge_side_panel": {
    "preferred_width": 450
